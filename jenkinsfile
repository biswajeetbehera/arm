pipeline {
    agent any

    stages {

        stage ('compile statge') {

            steps {
                withMaven(maven: 'maven_3_5_0') {
                    sh 'mvn clean compile'
                }
            }
        }
         
        stage ('test statge') {

            steps {
                withMaven(maven : 'maven_3_5_0') {
                    sh 'mvn test'
                }
            }
        }
        stage ('Deployment statge') {

            steps {
                withMaven(maven : 'maven_3_5_0') {
                    sh 'mvn deploy'
                }
            }
        }      
    }
}      
